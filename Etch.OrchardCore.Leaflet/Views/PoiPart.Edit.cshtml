@model Etch.OrchardCore.Leaflet.ViewModels.PoiPartEditViewModel

<div class="form-group" asp-validation-class-for="Latitude">
    <label asp-for="Latitude">@T["Latitude"]</label>
    <input asp-for="Latitude" class="form-control" readonly="readonly" />
    <span asp-validation-for="Latitude"></span>
</div>

<div class="form-group" asp-validation-class-for="Longitude">
    <label asp-for="Longitude">@T["Longitude"]</label>
    <input asp-for="Longitude" class="form-control" readonly="readonly" />
    <span asp-validation-for="Longitude"></span>
</div>

<p class="hint">Change position of POI by dragging marker on map.</p>